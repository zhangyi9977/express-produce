<!DOCTYPE html>
<html>
  <head>
    <title>首页</title>
    <% include head %>
  </head>
  <body>

    <%- include('bar',{username:data.username}) %>

    <div class="list">
      <% data.list.map((ele,idx)=>{ %>
        <div class="row">
            <span><%= idx+1 %></span>
            <span><a href="/detail?time=<%= ele.time %>"><%= ele.title %></a></span>
            <span><%= ele.username %></span>
            <span><%= ele.timeZ %></span>
            <span>
              <a href="/write?time=<%= ele.time %>">编辑</a>
              <a href="/articles/delete?time=<%= ele.time%>">删除</a>
            </span>
          </div>
         <%   }) %>
      

    <div class="pages">
      <% for(let i=1;i<=data.total;i++){ %>
        <a href="/?page=<%= i  %>"><%= i %></a>
        <%  }  %>
        
    </div>
  </div>
  </body>
</html>
